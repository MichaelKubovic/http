{
    "name": "bittyphp/bitty-http",
    "description": "PSR-7 and PSR-17 HTTP implementation.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Luke Kingsley",
            "email": "bizurkur@gmail.com"
        }
    ],
    "config": {
        "platform": {
            "php": "7.0.0"
        }
    },
    "require": {
        "php": ">=7.0.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0",
        "bittyphp/bitty-collection": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Bitty\\Http\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bitty\\Tests\\Http\\": "tests/"
        }
    },
    "minimum-stability": "dev"
}
